vehicleModelSelection2x2_postInitialize = function() {
var vehicleModelSelection;

vehicleModelSelection = bmw.shops.pageElements.VehicleModelSelection($('.vehicleSelectionTeaser2x2'));

var currentLevel = vehicleModelSelection.getCurrentLevel();
var progressBarValue = 70;

if (currentLevel == 0)
{
progressBarValue = 0;
}

if (currentLevel == 1)
{
progressBarValue = 25;
}

if (currentLevel == 2)
{
progressBarValue = 50;
}

$(".ui-progressbar-value").css('width', progressBarValue + "%");
$(".ui-progressbar-value").css('display', 'block');
$(".progress_step").text(progressBarValue + "%");
if (progressBarValue == 0) {
$("a.previousSelection").hide();
}
};
