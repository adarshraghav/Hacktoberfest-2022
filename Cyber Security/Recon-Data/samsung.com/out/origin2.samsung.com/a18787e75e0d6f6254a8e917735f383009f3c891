<script type="text/javascript">

visual_preview.prototype.invoke_item = function(anchor)
{
  var link = new fw_link(anchor.getAttribute("id"));
  var url = new fw_url_bean(link.get_url());
  if (link.get_target() == "_hidden")
  {
    if (link.get_add_properties_to_url())
    {
      url.set_parameter("done_page", 
                        "/iw-cc/base/util/refresh.html");
      url.set_parameter("full_redirect", true);
      link.set_url(url.get_url());
    }
   
    var hidden_frame = document.createElement("iframe");
    hidden_frame.style.visibility = "hidden";
    hidden_frame.style.width = "0px";
    hidden_frame.style.height = "0px";
    hidden_frame.src = link.get_url();
    window.document.body.appendChild(hidden_frame);
  }
  else if (link.get_target() == "_blank")
  {
    fw_widgets.open_new_window(anchor);
  }
  else
  {
    if (link.get_add_properties_to_url()) 
    { 
      var done_url = new fw_url_bean("/command/iw.group.sce_done");
      var vpath = '//cms_vip/america/main/ar/web/WORKAREA/manual/ar/vancouver2010/index.html';
      if (window.sce_is_frameset_mode && window.sce_is_frameset_mode())
      {
        vpath = top.sce_get_vpath();
      }
      done_url.set_parameter("vpath", vpath);
  
      if (window.location.search)
      {
        done_url.add_parameters(window.location.search);
      }
      url.set_parameter("done_page", done_url.get_url());
      url.set_parameter("full_redirect", false);
    }
    //need to always hit top since we may be in a frameset
    url.do_get(top);
  }
  return false;
}

function iw_refresh()
{
  var done_url = new fw_url_bean("/iw-cc/command/iw.group.preview_file");
  done_url.set_parameter("vpath",
                         "//cms_vip/america/main/ar/web/WORKAREA/manual/ar/vancouver2010/index.html");
  if (window.location.search)
  {
    done_url.add_parameters(window.location.search);
  }
  window.location = done_url.get_url();
}

</script>


