<script type="text/javascript">

function sce_get_vpath()
{
  return '//cms_vip/america/main/ar/web/WORKAREA/manual/ar/vancouver2010/index.html';
}

function sce_is_frameset_mode()
{
  return top != self && top.sce_is_frameset_mode && top.sce_is_frameset_mode();
}

function sce_image(image)
{
    this.src = image.src;
    this.width = image.width;
    this.height = image.height;
    this.alt = image.alt;
}

function sce_get_images()
{
    var imgsrcs = new Array();
    var index = 0;
    for(var i = 0; i < document.images.length; i++) 
    { 
        var image = document.images[i];
        if(sce_is_internal_url(image.src)) 
        {
            continue;
        }
        imgsrcs[index++] = new sce_image(image);
    }
    return imgsrcs;
}

function sce_is_internal_url(url)
{
    return url.indexOf("/iw-cc/") >= 0 || 
           url.match(/^javascript:.*(fw|iw|sce)_.*$/);
}

function sce_link(src, is_image)
{
    this.src = src;
    this.is_image = is_image;
}

function sce_get_links()
{
    var result = new Array();
    var index = 0;
    for(var i = 0; i < document.links.length; i++) 
    {
        if(sce_is_internal_url(document.links[i].href))
        {
             continue;
        }
        result[index++] = new sce_link(document.links[i].href, false);
    }
    for(var i = 0; i < document.images.length; i++)
    {
        if(sce_is_internal_url(document.images[i].src))
        {
             continue;
        }
        result[index++] = new sce_link(document.images[i].src, true);
    }
    return result;
}

window.screenX = window.screenX; // A silient workaround for Netscape 6.2.3 bug hanging sce
</script>
</body>
</html>